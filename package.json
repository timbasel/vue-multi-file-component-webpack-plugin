{
  "name": "vue-multi-file-component-webpack-plugin",
  "version": "1.0.0",
  "description": "The Vue Multi-File Component Webpack Plugin allows you to split Vue's Single-File Components into separate component files and have still have them loaded by the regular vue-loader",
  "repository": "git@github.com:timbasel/vue-multi-file-component-webpack-plugin.git",
  "author": {
    "name": "TimBasel",
    "email": "tim@basel-berlin.de"
  },
  "license": "MIT",
  "main": "dist/plugin.js",
  "scripts": {
    "clean": "del dist/",
    "build": "yarn run clean && tsc",
    "watch": "yarn run clean && tsc -w"
  },
  "dependencies": {
    "loader-utils": "^1.2.3",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "@types/loader-utils": "^1.1.3",
    "del-cli": "^1.1.0",
    "typescript": "^3.3.3333"
  }
}
